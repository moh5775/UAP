package Lab3;

public class UapBazar {
      public static void main(String[] args)
      {
    	  Product product = new Product("iPhone","20013","phone",12000.0);
    	 
    	  product.updatePrice(3000);
    	  System.out.print("Updated Price: "+ product.getPrice());
    	 
    	  System.out.print("\nDiscounted price: "+product.getDiscountedPrice(10));
    	 
    	  product.display();
      }
}
